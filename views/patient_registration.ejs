                <div class="content-wrapper">
                    <h3 class="page-title">Patient List</h3>
                    <div class="row mb-2">
                        <div class="col-lg-12">
                            <div class="card">
                                <div class="card-body">

                                    <form class="forms-sample" id="reg-form">
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Patient Name:</label>
                                            <input type="text" class="form-control p-input" id="name" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">DOB:</label>
                                            <input type="date" class="form-control p-input" id="age" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Education:</label>
                                            <input type="text" class="form-control p-input" id="edu" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Occupation:</label>
                                            <input type="text" class="form-control p-input" id="occu" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Contact No.:</label>
                                            <input type="text" class="form-control p-input" id="contact" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Address:</label>
                                            <input type="text" class="form-control p-input" id="address" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Date of Conceiving:</label>
                                            <input type="text" class="form-control p-input" id="doc" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Expected Delivery Date:</label>
                                            <input type="text" class="form-control p-input" id="dod" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">No. of Children:</label>
                                            <input type="text" class="form-control p-input" id="noc" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Complications:</label>
                                            <input type="text" class="form-control p-input" id="comp" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Blood Group:</label>
                                            <input type="text" class="form-control p-input" id="blgrp" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Height:</label>
                                            <input type="text" class="form-control p-input" id="ht" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Weight:</label>
                                            <input type="text" class="form-control p-input" id="wt" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Allergy:</label>
                                            <input type="text" class="form-control p-input" id="alleg" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Remarks:</label>
                                            <input type="text" class="form-control p-input" id="rem" aria-describedby="emailHelp" placeholder="Enter email">
                                        </div>

                                        <div class="col-12">
                                            <button type="submit" class="btn btn-primary" id="submit">Submit</button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

